{% extends "base.njk" %}

{% block styles %}
{% endblock %}

{% block content %}
<div class="content">

	<h1 class="centered">{{title}}</h1>
	<hr />
	<div class="centered">
		{{ contents | safe }}
	</div>

	<div>
		{% for project in collections.projects -%}
		<h3>{{project.title}}</h3>
		{{ project.contents | safe }}
		{% endfor -%}
	</div>

</div>
{% endblock %}
